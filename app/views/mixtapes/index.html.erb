<h1>Mixtapes#index</h1>


<div>
    <H1> WELCOME BACK: <%= @current_user.username %> </H1>

</div>

<div class= container>
<h3> List of Mixtapes uploaded </h3>
<ul>
<% @mixtapes.each do |tap| %>
    <li> <%= link_to(tap.title, user_mixtape_path(tap.user, tap)) %> - created by: <%=tap.user.name %> <br> uploaded - <%=tap.uploaded_at %> </li>
<% end %>
</ul>
</div>